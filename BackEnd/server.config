[server]
listen: localhost
webport: 8878
tcpport: 8879
logging_level: debug
# max_logsize should be defined as bytes
max_logsize: 52428800
# if True, the log will also be sent to stdout
log_to_stdout: True
max_connections_per_client: 3

[locations]
log: log

[ldap]
host: example.net
port: 10389
user: mcs
pass: mcs_pswd2
dc: dc=example, dc=com
radius_host: localhost
radius_secret: testing123

[clients]
whitelist: mcs,ums

# One section for each whitelisted client
[mcs]
id: 1
ip: localhost

[ums]
id: 2
ip: localhost